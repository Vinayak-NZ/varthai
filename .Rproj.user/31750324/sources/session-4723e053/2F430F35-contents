<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>

<script>
  document.addEventListener("DOMContentLoaded", function() {
    new Typed("#typed-output", {
      strings: ["Varthai"],
      typeSpeed: 150,
      startDelay: 500,
      showCursor: true,
      cursorChar: '|',
      loop: false
    });
  });
</script>